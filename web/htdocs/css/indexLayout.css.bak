input[type='text'], input[type='email'], input[type='password'], input[type='text']
{
	font-size: 13px;
	font-family: '±¼¸²';
	padding: 2px;
	padding-left: 5px;
	margin: 3px;
	border: 1px solid #ccc;
	height: 25px;
	outline-style: none;

	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;

	-moz-transition: 0.3s all ease;
    -ms-transition: 0.3s all ease;
    -o-transition: 0.3s all ease;
    -webkit-transition: 0.3s all ease;
    transition: 0.3s all ease;
}
input[type='email']:focus, input[type='password']:focus, input[type='text']:focus
{
	-moz-box-shadow: 0 0 7px rgba(61,183,204,0.7);
	-webkit-box-shadow: 0 0 7px rgba(61,183,204,0.7);
	box-shadow: 0 0 7px rgba(61,183,204,0.7);
}

input[type='email'], input[type='text']
{
	width: 290px;
}
input[type='password']
{
	width: 200px;
}

.submit
{
	font-size: 9pt;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	width: 80px;
	height: 30px;

	-moz-transition: 0.8s all ease;
    -ms-transition: 0.8s all ease;
    -o-transition: 0.8s all ease;
    -webkit-transition: 0.8s all ease;
    transition: 0.8s all ease;

	border: solid 1px #5f9ec1;

	text-shadow: 0px 1px 1px #4c84a3;
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #86c4e7), color-stop(100%, #67aad1));
	background: -webkit-linear-gradient(#86c4e7, #67aad1);
	background: -moz-linear-gradient(#86c4e7, #67aad1);
	background: -o-linear-gradient(#86c4e7, #67aad1);
	-pie-background: linear-gradient(#86c4e7,#67aad1);
	background: linear-gradient(#86c4e7,#67aad1);

	color: white;
	font-weight: bold;
	font-family: "±¼¸²", Gothic;
}
.submit:hover
{
	text-shadow: 0px 1px 1px #4c84a3;
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #96d4f7), color-stop(100%, #77bae1));
	background: -webkit-linear-gradient(#96d4f7, #77bae1);
	background: -moz-linear-gradient(#96d4f7, #77bae1);
	background: -o-linear-gradient(#96d4f7, #77bae1);
	-pie-background: linear-gradient(#96d4f7,#77bae1);
	background: linear-gradient(#96d4f7,#77bae1);

	-moz-box-shadow: 0 0 7px #6799FF;
	-webkit-box-shadow: 0 0 7px #6799FF;
	box-shadow: 0 0 10px #6799FF;
}

.submit:active
{
	text-shadow: 0px 1px 1px #4c84a3;
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #67aad1), color-stop(100%, #86c4e7));
	background: -webkit-linear-gradient(#67aad1, #86c4e7);
	background: -moz-linear-gradient(#67aad1, #86c4e7);
	background: -o-linear-gradient(#67aad1, #86c4e7);
	-pie-background: linear-gradient(#67aad1,#86c4e7);
	background: linear-gradient(#67aad1,#86c4e7);
}

